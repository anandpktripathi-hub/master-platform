    "@types/express-rate-limit": "^5.1.3",
    "@types/speakeasy": "^2.0.10",
    "aws-sdk": "^1.18.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "csv-parse": "^6.1.0",
