{
  "name": "master-backend-clean",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "node -e \"console.log('[root] This repo uses ./backend as the canonical backend for Docker and local runs.')\" && nest build",
    "backend:build": "cd backend && npm run build",
    "backend:dev": "cd backend && npm run start:dev",
    "backend:prod": "cd backend && npm run build && npm run start:prod",
    "backend:dev:with-db": "cd backend && npm run start:dev:with-db",
    "frontend:dev": "cd frontend && npm run dev",
    "postbuild": "node scripts/copy-env-to-dist.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "node scripts/start-root-dev.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm run build && node dist/main.js",
    "dev:db": "docker compose up -d mongodb",
    "dev:redis": "docker compose up -d redis",
    "start:dev:with-db": "npm run dev:db || echo \"[dev:db] Docker not available or not running. Please ensure MongoDB is running on localhost:27017. Continuing to start backend...\" && npm run start:dev",
    "start:dev:with-services": "npm run dev:db && npm run dev:redis || echo \"[dev:services] Docker or services not available. Ensure MongoDB and Redis are running on localhost (27017, 6379). Continuing to start backend...\" && npm run start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "audit:admin-nav": "node -e \"console.log('Removed: audit script was deleted to avoid heavy file scanning in this workspace.')\"",
    "report:launch-blockers": "node -e \"console.log('Removed: report script was deleted to avoid heavy file scanning in this workspace.')\""
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/schedule": "^2.0.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/terminus": "^11.0.0",
